DEF click TouchSensor {enabled TRUE }
DEF	timer TimeSensor {
	cycleInterval 5
	loop FALSE
	enabled FALSE
	
}
DEF	timer1 TimeSensor {
	cycleInterval 5
	loop FALSE
	enabled FALSE
	
}
DEF	quay1 OrientationInterpolator {
	key [0 0.5 1]
	keyValue [0 1 0 0,0 1 0 3.14,0 1 0 6.28]
}
DEF	quay2 OrientationInterpolator {
	key [0 0.5 1]
	keyValue [ 0 1 0 0,0 1 0 -3.14,0 1 0 -6.28]
}
DEF ma Script {

	eventIn SFBool turnOn

	eventOut SFBool On

	eventOut SFBool Off

	field SFBool turn FALSE

	url "javascript:

	function turnOn(val){

		if(val){

			turn = !turn;

			if(turn)

			{	  
				On= TRUE;  
                Off=FALSE;
			}
			else
			{
				On=FALSE;
				Off=TRUE;  
        	}  	
		}

    }"

}


				key [0 0.5 1]
				keyValue [ 5.3 5 -1,5.4 5.5 -1,5.5 6 -1]
			}
DEF	thanh3x PositionInterpolator {
				key [0 0.5 1]
				keyValue [9.5 5 -1,9 4.5 -1 ,8.5 4 -1]
			}
DEF	thanh3l PositionInterpolator {
				key [0 0.5 1]
				keyValue [ 8.5 4 -1,9 4.5 -1, 9.5 5 -1]
			}
DEF	gaux PositionInterpolator {
				key [0 0.5 1]
